{
  "version": 2,
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "vite.config.ts", "use": "@vercel/static-build" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [{ "src": "/api/(.*)", "dest": "/api/$1.ts" }, { "handle": "filesystem" }]
}
